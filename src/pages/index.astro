---
import Layout from '@/layouts/Layout.astro'

const currentUrl = Astro.url;
const title = "Practise Patriarchal Zen";
const imgSrc = "/images/white-silk-sheet/white-silk-sheet-1.png";
const alt = 'White Silk Sheet';
---

<Layout canonicalUrl={currentUrl} title={title}>
  <section class="flex flex-col gap-y-12 px-4 py-8 sm:px-8 lg:px-16">

    <img src={imgSrc} alt={alt} />

  </section>
</Layout>
